{
  "name": "github-search-api",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.11",
    "@types/jest": "24.0.13",
    "@types/node": "12.0.2",
    "@types/react": "16.8.18",
    "@types/react-dom": "16.8.4",
    "mobx": "^5.9.4",
    "mobx-react": "^5.4.4",
    "mobx-react-devtools": "^6.1.1",
    "mobx-state-tree": "^3.14.0",
    "polished": "^3.3.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "babel-plugin-emotion": "^10.0.9",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "serve": "^11.0.0",
    "source-map-explorer": "^1.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:all": "CI=true yarn run test --bail --colors --ci",
    "eject": "react-scripts eject",
    "serve": "serve --no-clipboard --single",
    "explore-source-maps": "source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "last 1 chrome version",
    "last 1 firefox version",
    "last 1 safari version"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run test:all && yarn run build"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "homepage": "."
}
